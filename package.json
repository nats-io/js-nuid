{
  "name": "js-nuid",
  "version": "1.1.0-2",
  "description": "Nuid for javascript",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run package",
    "cover": "nyc report --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "clean": "rm -rf lib",
    "test": "npm run build && nyc ava --verbose -T 15000",
    "debugtest": "tsc && node $NODE_DEBUG_OPTION node_modules/.bin/ava --verbose -T 6500000 -m",
    "package": "webpack-cli && cp lib/nuid.js index.js && cp lib/src/nuid.d.ts index.d.ts && cp lib/src/nuid.js.map nuid.js.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//nats-io/js-nuid"
  },
  "keywords": [
    "nuid",
    "nats-io"
  ],
  "author": "NATS Authors",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^13.1.7",
    "ava": "^2.4.0",
    "browser-env": "^3.3.0",
    "coveralls": "^3.0.9",
    "jsdom": "^16.0.0",
    "nyc": "^15.0.0",
    "serialize-javascript": "^2.1.2",
    "typescript": "^3.7.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10"
  },
  "ava": {
    "require": [
      "./lib/test/helpers/setup-browser-env.js",
      "./lib/test/helpers/ava_fix.js"
    ],
    "files": [
      "./lib/test/**/*.js",
      "!./lib/test/helpers/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "node": "^12.11.1",
    "ts-loader": "^6.2.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "src/**/*.ts",
      "lib/src/**/*.js"
    ],
    "exclude": [
      "lib/test/**",
      "examples/**",
      "benchmark/**"
    ],
    "sourceMap": true,
    "all": true
  }
}
